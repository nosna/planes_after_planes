{
  "manifest_version": 2,
  "name": "planes_after_planes",
  "version": "1.0",
  "description": "nonestop to get ur ticket",
  "icons": {
    "16": "img/plane.png",
    "64": "img/plane.png",
    "128": "img/plane.png"
  },
  "browser_action": {
    "default_title": "planes_after_planes",
    "default_popup": "popup.html"
  },


  "background": {
   "scripts": ["js/background.js"],
   "persistant": false
  },

  "permissions": ["notifications", "tabs", "storage",
    "https://www.xiamenair.com/*"
    ],

  "content_scripts": [{
    "matches": [
      "https://www.xiamenair.com/*"
    ],
    "js": ["js/content.js"]
  }]
}
